<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:id="@+id/LinearLayout3"
              android:weightSum="300"
              android:layout_width="fill_parent"
              android:layout_height="fill_parent"
              android:orientation="horizontal">
  <TextView
      android:id="@+id/tvSeatReportFrom"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:padding="10dp"
      android:layout_weight="51"
      android:textSize="12sp">
  </TextView>

  <TextView
      android:id="@+id/tvSeatReportTo"
      android:layout_weight="51"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:padding="10dp"
      android:textSize="12sp">
  </TextView>

  <TextView
      android:id="@+id/tvSeatReportTime"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:padding="10dp"
      android:layout_weight="51"
      android:textSize="12sp">
  </TextView>

  <TextView
      android:id="@+id/tvSeatReportEconomy"
      android:layout_weight="49"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:padding="10dp"
      android:textSize="12sp">
  </TextView>

  <TextView
      android:id="@+id/tvSeatReportBusiness"
      android:layout_weight="49"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:padding="10dp"
      android:textSize="12sp">
  </TextView>

  <TextView
      android:id="@+id/tvSeatReportFirst"
      android:layout_weight="49"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:padding="10dp"
      android:textSize="12sp">
  </TextView>

</LinearLayout>
